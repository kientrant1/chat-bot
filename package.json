{
  "name": "chat-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "package:audit": "yarn npm audit",
    "prepare": "husky",
    "prisma:init": "prisma init",
    "prisma:generate": "prisma generate --no-engine",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev --name",
    "prisma:reset": "prisma migrate reset",
    "prebuild": "yarn prisma:generate",
    "postinstall": "yarn prisma:generate"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.19.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.83.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "firebase": "^12.5.0",
    "newrelic": "^13.6.5",
    "next": "^16.0.7",
    "next-auth": "^4.24.13",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "resend": "^6.5.2",
    "snackact-ui": "^0.0.10",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6",
    "@types/newrelic": "^9.14.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "prisma": "^7.1.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "packageManager": "yarn@4.10.3",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
